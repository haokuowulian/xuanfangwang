.page{
  width: 680rpx;
  height: 100vh;
  padding-left: 35rpx;
  padding-right: 35rpx;
  background-color: #ffffff;
}
.page-title{
  font-size: 50rpx;
  color: #101010;
  margin-top: 80rpx;
  margin-bottom: 45rpx;
}
.info{
  height: 130rpx;
  width: 680rpx;
  border-bottom: 1rpx solid #f7f7f7;
  display: flex;
  flex-direction: column;
  padding-top: 35rpx;
}
.info1{
  height: 250rpx;
}
.text_input{
  width: 680rpx;
  color: #888888;
  font-size: 30rpx;
}
.text_input1{
  width: 585rpx;
  padding-left: 15rpx;
  color: #919191;
  font-size: 30rpx;
}
.info-title{
  font-size: 35rpx;
  color: #333333;
}
.info-content{
  margin-top: 20rpx;
}
.info-content1{
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.right-img{
  height: 19rpx;
  width: 10rpx;
}
.info-content2{
  height: 180rpx;
  width: 200rpx;
  margin-left: 25rpx;
  margin-top: 25rpx;
  background-color: #f7f7f7;
  position: relative;
}
.img{
  height: 180rpx;
  width: 200rpx;
}
.delImg{
  position: absolute; top: -20rpx; right: -20rpx; height: 18px; overflow: visible
}
.row-extra{
  width: 650rpx;
  padding-left: 15rpx;
  padding-right: 25rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  color: #919191;
}
.page-btn{
  position:fixed;
  bottom:0;
  height: 110rpx;
  width: 750rpx;
  background-color: #ff9900;
  font-size: 35rpx;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}